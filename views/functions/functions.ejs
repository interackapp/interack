        <%= "https://www.pinterest.com/"+value %>
    <% }  else if(tag === 'snapchat'){ %>
        <%= "https://www.snapchat.com/add/"+value %>
    <% }  else if(tag === 'tiktok'){ 
        if(value.includes("@")) { %>
            <%= "https://www.tiktok.com/"+value %>
        <% }else { %>
            <%= "https://www.tiktok.com/@"+value %>
        <% } %>
    <% } else if(tag === 'twitch'){ %>
        <%= "https://www.twitch.tv/"+value %>
