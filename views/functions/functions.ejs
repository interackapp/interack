<% getLink = function(value, tag){
    if(tag === 'linkedin') { 
        if(value.includes("http")) { %>
            <%= value %>
        <% }else { %>
            <%= "https://"+value %>
        <% } %>
    <% } else if(tag === 'instagram'){ %>
        <%= "https://www.instagram.com/"+value %>
    <% } else if(tag === 'facebook'){ 
        if(value.includes("http")) { %>
            <%= value %>
        <% }else { %>
            <%= "https://"+value %>
        <% } %>
    <% }  else if(tag === 'youtube'){ 
        if(value.includes("http")) { %>
            <%= value %>
        <% }else { %>
            <%= "https://"+value %>
